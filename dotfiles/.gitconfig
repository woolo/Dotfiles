[gpg]
	program = qubes-gpg-client-wrapper

[commit]
	gpgsign = true

[alias]
	stag = "!id=`git rev-parse --verify HEAD`; git tag -s user_${id:0:8} -m \"Tag for commit $id\""
	vtag = !git tag -v `git describe`
[core]
	editor = emacs
	excludesfile = /home/user/.gitignore_global
